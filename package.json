{
    "private": true,
    "name": "nft-core",
    "version": "1.0.0-beta.28",
    "description": "NFT Functionality For Any ARK Core-v3 Bridgechain",
    "engineStrict": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/protokol/nft-plugins.git"
    },
    "keywords": [
        "protokol",
        "ark",
        "core",
        "sdk",
        "nft",
        "blockchain"
    ],
    "scripts": {
        "build": "cross-env bash ./scripts/build.sh",
        "publish:beta": "cross-env-shell bash ./scripts/publish-beta.sh",
        "publish:latest": "cross-env-shell bash ./scripts/publish-latest.sh",
        "test:functional": "cross-env-shell bash ./scripts/test-functional.sh",
        "test:integration": "cross-env-shell bash ./scripts/test-integration.sh",
        "test:unit": "cross-env-shell bash ./scripts/test-unit.sh",
        "test": "yarn test:unit && yarn test:functional && yarn test:integration"
    },
    "devDependencies": {
        "cross-env": "^7.0.2"
    }
}
